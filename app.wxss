/** Layout**/
.normal-flex{
  display: flex;
}

.flex-column{
  display: flex;
  flex-direction: column;
}

.flex-column-space-between{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.normal-inline-block{
  display: inline-block;
}

.margin-all-5{
  margin:5rpx;
}

.margin-all-10{
  margin:10rpx;
}