/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.title{
  font-size: 55rpx;
}
.appName{
  color: #558ED5;
  font-weight: bold;
  font-size: 60rpx;
}

.saceosLogo{
  width: 200rpx;
  height:100rpx;
  float: right;
}

.hr{
  height: 5rpx;
  border-bottom: 1px solid white;
  width:90%;
  margin-left:10%;
}

.icon {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
}

